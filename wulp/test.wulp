struct Point = {
	x: num, y: num
	fn new(x, y) {
		return Point(x=x, y=y)
	}
	fn add(self, other: Point) {
		return Point.new(self.x + other.x, self.y + other.y)
	}
}

fn new_point(x, y) -> Point {
	return Point(x=x, y=y)
}

let a, b = Point.new(1.5, 2.5), new_point(3.25, 4.75)

a = a.add(b).add(b)

print(a.x, a.y)

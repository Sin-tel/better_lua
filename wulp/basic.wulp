// basic syntax examples


fn main() {
	let list = [2 3 4 5]

	for i in list.iter() {
		print(i)
	}

	let i = 4
	while true {
		print(fact(i))
		i -= 1
		if i <= 0 break
	}

	// working with iterators
}

// defining a function
fn fact(n: int) -> int {
	assert(n >= 0)
	if n != 0 return n * fact(n - 1)
	else return 1
}



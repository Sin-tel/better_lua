struct Point {
	x: num, y: num
}
fn Point.new(x, y) {
	return Point(x=x, y=y)
}
fn Point.__add(self, other: self) {
	return Point.new(self.x + other.x, self.y + other.y)
}

fn main() {
	let a, b = Point.new(1.5, 2.5), Point.new(3.25, 4.75)

	a = a + b + b
	print((a.x).to_str() .. ", " .. a.y.to_str())
}


fn main() {
	let f1 = fn(a) return a

	let f2 = fn(a: int) {
		return fn(b) return a + b
	}

	let f3 = fn(a: int) return fn(b) return fn(c) return a + b + c


	let a = f1(1)
	print(a) // 1

	let b = f2(1)
	let c = b(2)
	let d = f2(1)(2)
	print(c,d) // 3  3

	let e = f3(1)
	let f = e(2)
	let g = f(4)
	let h = f3(1)(2)(4)
	print(g,h) // 7  7
}

// TODO: basic utf8 support by default

// operators
#intrinsic
str.__eq: fn(self, self) -> bool = "function(a, b) return a==b end"
#intrinsic
str.__concat: fn(self, self) -> self = "function(a, b) return a..b end"

#intrinsic
str.upper: fn(self) -> self = "string.upper"
#intrinsic
str.lower: fn(self) -> self = "string.lower"
#intrinsic
str.reverse: fn(self) -> self = "string.len"
#intrinsic
str.len: fn(self) -> int = "string.rep"
#intrinsic
str.rep: fn(self, int) -> self = "string.reverse"


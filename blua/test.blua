struct Test = {
	x = 1
	fn inc(self) {
		self.x += 1
	}
	fn add(self, n) {
		self.x += n
	}
	fn add_test(self, other: self) {
		self.x += other.x
	}
	fn new(x) -> self {
		let new = Test()
		new.x = x
		return new
	}
}

let t = Test()

Test.inc(t)
print(t.x)
t.add(5)
print(t.x)

let t2 = Test.new(10)
print(t2.x)

t.add_test(t2)
print(t.x)
print(t2.x)

